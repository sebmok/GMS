<!DOCTYPE html>
<html lang="en">
<head>
    <base href="/" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0" />

    <!-- core scripts -->
    <script src="~/Scripts/jquery-2.2.4.js"></script>      
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>
    <script src="~/Scripts/angular-loader.js"></script> 
    <script src="~/Scripts/angular-resource.js"></script>
    <script src="~/Scripts/angular-route.js"></script>
    <script src="~/Scripts/angular-sanitize.js"></script>
    <script src="~/Scripts/respond.js"></script>

    <!-- app scripts -->
    <script src="~/Content/SYSTEM/app/app.js"></script>
    <script src="~/Content/SYSTEM/app/login/loginCtrl.js"></script>
    <script src="~/Content/SYSTEM/common/configuration.js"></script>
    <script src="~/Content/SYSTEM/common/common.core.js"></script>
    <script src="~/Content/SYSTEM/common/common.ui.js"></script>
    <script src="~/Content/SYSTEM/app/layout/dashboard/sidebarController.js"></script>
    <script src="~/Content/SYSTEM/app/layout/dashboard/sideBarDirective.js"></script>
    <script src="~/Content/SYSTEM/app/layout/dashboard/topBarController.js"></script>
    <script src="~/Content/SYSTEM/app/layout/dashboard/topBarDirective.js"></script>
    <script src="~/Content/SYSTEM/common/custom.js"></script>



    <!-- Auth0 Lock script and AngularJS module -->
    <script src="//cdn.auth0.com/js/lock-9.1.min.js"></script>
    <!-- angular-jwt and angular-storage -->
    <script type="text/javascript" src="//cdn.rawgit.com/auth0/angular-storage/master/dist/angular-storage.js"></script>
    <script type="text/javascript" src="//cdn.rawgit.com/auth0/angular-jwt/master/dist/angular-jwt.js"></script>
    <script src="//cdn.auth0.com/w2/auth0-angular-4.js"> </script>
     
    <!-- Custom styling plus plugins -->
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/SYSTEM/css/custom.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
  
    <!--[if lt IE 9]>
    <script src="../assets/js/ie8-responsive-file-warning.js"></script>
    <![endif]-->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body class="nav-md" ng-app="app">
    <div class="container body">
        <div class="main_container">           
            <!-- sidebars -->
            <div class="col-md-3 left_col" ng-controller="sidebarCtrl">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <!--<a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>Empty!</span></a>-->
                    </div>
                    <div class="clearfix"></div>

                    <!-- /menu prile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">    
                            <script type="text/ng-template" id="treeMenu">                             
                                <a><i class="fa fa-edit"></i>{{menu.navName}}<span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none" ng-if="(SiteMenu | filter:{parent : menu.muncui}).length > 0">
                                        <li ng-repeat="menu in SiteMenu | filter:{parent : menu.muncui}" ng-include="'treeMenu'">
                                        </li>
                                    </ul>                               
                            </script>   
                            
                            <ul class="nav side-menu">
                                <li>
                                    <a><i class="fa fa-edit"></i> Menu2 <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu" style="display: none">
                                        <li>
                                            <a href="empty.html">Menu2.1</a>
                                        </li>
                                        <li>
                                            <a href="empty.html">Meny2.2s</a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                                        
                            
                            <ul class="nav side-menu">
                                <li ng-repeat="menu in SiteMenu | filter: {parent : '!'}">
                                    <a><i class="fa fa-edit"></i>{{menu.navName}}<span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu" style="display: none">
                                           <li  ng-repeat="menu in SiteMenu | filter:{parent : menu.muncui}">
                                           </li>
                                        </ul>
                                </li>
                            </ul>
                        </div>


                        <div class="menu_section">
                            <script type="text/ng-template" id="treeMenu">
                                <a href="">{{menu.navName}}</a>
                                <ul class="nav child_menu" ng-if="(SiteMenu | filter:{parent : menu.muncui}).length > 0">
                                    <li ng-repeat="menu in SiteMenu | filter:{parent : menu.muncui} : true" ng-include="'treeMenu'"></li>
                                </ul>
                            </script>

                            <ul class="nav side-menu">
                                <!--@* Here we will load only top level menu *@-->
                                <li ng-repeat="menu in SiteMenu | filter: {parent : '!'}" ng-include="'treeMenu'"></li>
                            </ul>
                        </div>
                    </div>
                    <!-- /sidebar menu -->
                    <!-- menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Settings">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>


           
            <!-- top navigation -->
            <div class="top_nav">
               <topbar></topbar>
            </div>
            <!-- /top navigation -->

            <!-- page content -->
            <div class="right_col" role="main">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        @RenderBody()
                    </div>
                </div>               
            </div>          
            <!-- /page content -->
        </div>
    </div>  
</body>
</html>
